#!/bin/sh

set -e

case "$1" in
    remove|purge)
	if [ -x /usr/bin/apt-key ]; then
	    if [ ! -z "`apt-key list | grep DFB0B268 2>/dev/null`" ]; then
		apt-key del DFB0B268
	    fi
	fi
    ;;

    upgrade|deconfigure)
    ;;

    failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
